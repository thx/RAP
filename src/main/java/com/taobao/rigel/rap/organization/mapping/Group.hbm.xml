<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.taobao.rigel.rap.organization.bo.Group" table="tb_group">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="name">
            <column name="name"/>
        </property>
        <property name="productionLineId">
            <column name="production_line_id"/>
        </property>
        <property name="userId">
            <column name="user_id"/>
        </property>
    </class>
</hibernate-mapping>